# nahd=100e-3, ksoma=4e-3
# makes rebound spike at end of long flash, which generates Ca spike
#
rcbp_chirp_model --cellnum 576 --n_dbp1 1 --dbp1_am 2 --n_dbp2 2 --dbp2_am 2 --amarr 22 --make_cones 1 --cone_type 2 --vnoise 0 --rvcloc 0 --drm 30e3 --psyn1 823 --psyn3 807 --axdia 0.7 --spotdur 1.0 --spotdia 250 --g_dbp1_am 1e-12 --g_am_dbp1 4e-10 --dbp1_gtau 1 --g_dbp1_am2 1e-12 --g_am2_dbp1 2e-10 --g_dbp2_am 7e-10 --g_am_dbp2 3e-10 --am_sdur2 500 --am_sfall2 0 --am_atau 1 --am_sfall 2 --g_dbp2_am2 4e-10 --g_am2_dbp2 4e-12 --mxrot 0 --ninfo 2 --comnd_line_only 0 --one_run 1 --disp_c2 1 --dri 200 --cmult 0.0012 --minten -0.041 --nahd 100e-3 --ksoma 4e-3 --cadist 1e-3 --v 1 --d 0 --w 1.24 &

# same, but cadist = 1.5e-3
#   increases [Ca] average level and also amplitude of Ca spike
#
rcbp_chirp_model --cellnum 576 --n_dbp1 1 --dbp1_am 2 --n_dbp2 2 --dbp2_am 2 --amarr 22 --make_cones 1 --cone_type 2 --vnoise 0 --rvcloc 0 --drm 30e3 --psyn1 823 --psyn3 807 --axdia 0.7 --spotdur 1.0 --spotdia 250 --g_dbp1_am 1e-12 --g_am_dbp1 4e-10 --dbp1_gtau 1 --g_dbp1_am2 1e-12 --g_am2_dbp1 2e-10 --g_dbp2_am 7e-10 --g_am_dbp2 3e-10 --am_sdur2 500 --am_sfall2 0 --am_atau 1 --am_sfall 2 --g_dbp2_am2 4e-10 --g_am2_dbp2 4e-12 --mxrot 0 --ninfo 2 --comnd_line_only 0 --one_run 1 --disp_c2 1 --dri 200 --cmult 0.0012 --minten -0.041 --nahd 100e-3 --ksoma 4e-3 --cadist 1.5e-3 --v 1 --d 0 --w 1.24 &

