# display the spot-chirp stimulus:
#
retsim --expt cbp_chirp --cbptype dbp1 --make_dbp1 1 --n_dbp1 100 --dbp1_densfile dens_cbp_chirp.n --dbp1_densfile2 dens_cbp_chirpc.n --dbp1_nscale -2.09 --am_nscale -2.09 --tip1 807 --presyn1 807 --remove_nconns 1 --make_dbp1_am 1 --ninfo 2 --amarr 2 --mxrot -0 --spotdia 200 -d 16 -v | vid

#
# run with ksoma, nahd defined
#
retsim --expt cbp_chirp --cbptype dbp1 --make_dbp1 1 --n_dbp1 100 --dbp1_densfile dens_cbp_chirp.n --dbp1_densfile2 dens_cbp_chirpc.n --ksoma 0.1e-3 --nahd 90e-3 --dbp1_nscale -2.09 --am_nscale -3.09 --tip1 807 --presyn1 807 --remove_nconns 1 --make_dbp1_am 1 --ninfo 2 --info_chan 1 --amarr 2 --mxrot -0 --spotdia 300 -v | vid 
#
# run with g_am_dbp1 = 0, ksoma, nahd defined
#
retsim --expt cbp_chirp --cbptype dbp1 --make_dbp1 1 --n_dbp1 100 --dbp1_densfile dens_cbp_chirp.n --dbp1_densfile2 dens_cbp_chirpc.n --ksoma 0.1e-3 --nahd 90e-3 --g_am_dbp1 0 --dbp1_nscale -2.09 --am_nscale -3.09 --tip1 807 --presyn1 807 --remove_nconns 1 --make_dbp1_am 1 --ninfo 2 --info_chan 1 --amarr 2 --mxrot -0 --spotdia 300 -v | vid 

#
# g_am_dbp1 = 5e-10, ksoma = nahd = 0
#
retsim --expt cbp_chirp --cbptype dbp1 --make_dbp1 1 --n_dbp1 100 --dbp1_densfile dens_cbp_chirp.n --dbp1_densfile2 dens_cbp_chirpc.n --ksoma 0 --nahd 0 --dbp1_nscale -2.09 --am_nscale -3.09 --tip1 807 --presyn1 807 --remove_nconns 1 --make_dbp1_am 1 --ninfo 2 --info_chan 1 --amarr 2 --mxrot -0 --g_am_dbp1 5e-10 --spotdia 300 -v | vid 

#
#
# g_am_dbp1 = 5e-10, ksoma = nahd = 0, uses predefined densfile, densfile2, make implicit
#
retsim --expt cbp_chirp --cbptype dbp1 --n_dbp1 100 --ksoma 0 --nahd 0 --dbp1_nscale -2.09 --am_nscale -3.09 --tip1 807 --tip2 803 --presyn1 807 --presyn2 803 --remove_nconns 1 --make_dbp1_am 1 --ninfo 2 --info_chan 1 --amarr 2 --mxrot -0 --g_am_dbp1 5e-10 --spotdia 300 -v | vid -c > cbp_amac2_feedback.ps

#
#  cbptype = dbp2, same as above
#
retsim --expt cbp_chirp --cbptype dbp2 --n_dbp2 100 --ksoma 0 --nahd 0 --dbp2_nscale -2.09 --am_nscale -3.09 --tip1 807 --tip2 803 --presyn1 807 --presyn2 803  --remove_nconns 1 --make_dbp2_am 1 --ninfo 2 --info_chan 0 --amarr 2 --mxrot -0 --g_am_dbp1 5e-10 --spotdia 300 -v | vid

#
# amarr = 21, 2 ams, 11 dbp1s, 10 dbp2s
#
retsim --expt cbp_chirp --cbptype dbp1 --cbptype2 dbp2 --n_dbp1 100 --n_dbp2 100 --ksoma 0 --nahd 0 --dbp1_nscale -3.09 --dbp2_nscale -3.09 --am_nscale -3.09 --tip1 807 --tip2 803 --presyn1 807 --presyn2 803 --remove_nconns 1  --ninfo 2 --info_chan 0 --amarr 21 --mxrot -0 --g_am_dbp1 5e-10 --spotdia 300 -d 1 -v | vid


#
# make just one dbp1
#
retsim --expt cbp_chirp --cbptype dbp1 --cbptype2 dbp2 --n_dbp1 100 --n_dbp2 100 --ksoma 0 --nahd 0 --dbp1_nscale -3.09 --dbp2_nscale -3.09 --am_nscale -3.09 --tip1 807 --tip2 803 --presyn1 807 --presyn2 803 --remove_nconns 1 --ninfo 2 --info_chan 0 --amarr 21 --mxrot -0 --g_am_dbp1 5e-10 --spotdia 300 --n_dbp2 0 --n_dbp1 1 --n_am 0 -d 1 -v | vid -w 1.5

#
# n_dbp1, n_dbp2 not set, make random arrays, 30 x 300 um
#
retsim --expt cbp_chirp --cbptype dbp1 --cbptype2 dbp2 --ksoma 0 --nahd 0 --dbp1_nscale -3.09 --dbp2_nscale -3.09 --am_nscale -3.09 --tip1 807 --tip2 803 --presyn1 807 --presyn2 803 --remove_nconns 1 --ninfo 2 --info_chan 0 --amarr 21 --mxrot -0 --g_am_dbp1 5e-10 --spotdia 300 -r 11112 -d 1 -v | vid

