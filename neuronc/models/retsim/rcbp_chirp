rcbp_chirp_model --cellnum 576 --n_dbp1 11 --axdia 1.0 --one_run 1
rcbp_chirp_model --cellnum 576 --n_dbp1 11 --axdia 0.6 --one_run 1 --vnoise 0
rcbp_chirp_model --cellnum 576 --n_dbp1 11 --axdia 1.0 --one_run 1 --vnoise 0

