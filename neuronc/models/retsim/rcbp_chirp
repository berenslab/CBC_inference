rcbp_chirp_model --cellnum 576 --n_dbp1 1 --nahd 0e-3 --ksoma 0e-3 --minten 18e-12 --cmult 9e-12 --elec_rs 1.5e9 --vcontrast 1 --d 0 --v 1

