# Test of T-type Ca channels

# catdist > 0, cadist=0
#  With _CA_T = CA7, offm = -0.010, reverses phase of [Ca],  glut release
#  Note that CA7 inactivation is not voltage-dependent.
#
rcbp_chirp_model --cellnum 576 --n_dbp1 1 --dbp1_am 2 --n_dbp2 2 --dbp2_am 2 --amarr 22 --make_cones 1 --cone_type 2 --vnoise 0 --rvcloc 0 --drm 30e3 --psyn1 823 --psyn3 807 --axdia 0.7 --spotdur 1.0 --spotdia 250 --g_dbp1_am 1e-12 --g_am_dbp1 4e-10 --dbp1_gtau 1 --g_dbp1_am2 1e-12 --g_am2_dbp1 2e-10 --g_dbp2_am 7e-10 --g_am_dbp2 3e-10 --am_sdur2 500 --am_sfall2 0 --am_atau 1 --am_sfall 2 --g_dbp2_am2 4e-10 --g_am2_dbp2 4e-12 --mxrot 0 --ninfo 2 --comnd_line_only 0 --one_run 1 --disp_c2 1 --dri 200 --cmult 0.0012 --minten -0.041 --naab 0e-3 --ksoma 0e-3 --cadist 0e-3 --catdist 0.05e-3 --v 1 --d 0 --w 1.24 &

# catdist > 0, cadist=0
#  With _CA_T = CA6, offm = 0.010, makes transients in [Ca],  glut release
#   As offm goes to -0.010 reverses phase, as offm >= 0.015, noninvert phase
#   Note that CA6 inactivation is not voltage-dependent.
#
rcbp_chirp_model --cellnum 576 --n_dbp1 1 --dbp1_am 2 --n_dbp2 2 --dbp2_am 2 --amarr 22 --make_cones 1 --cone_type 2 --vnoise 0 --rvcloc 0 --drm 30e3 --psyn1 823 --psyn3 807 --axdia 0.7 --spotdur 1.0 --spotdia 250 --g_dbp1_am 1e-12 --g_am_dbp1 4e-10 --dbp1_gtau 1 --g_dbp1_am2 1e-12 --g_am2_dbp1 2e-10 --g_dbp2_am 7e-10 --g_am_dbp2 3e-10 --am_sdur2 500 --am_sfall2 0 --am_atau 1 --am_sfall 2 --g_dbp2_am2 4e-10 --g_am2_dbp2 4e-12 --mxrot 0 --ninfo 2 --comnd_line_only 0 --one_run 1 --disp_c2 1 --dri 200 --cmult 0.0012 --minten -0.041 --naab 0e-3 --ksoma 0e-3 --cadist 0e-3 --catdist 0.1e-3 --v 1 --d 0 --w 1.24 &

#
# with larger signal (cmult 0.002), more depolarized cone (minten -0.039), gives Ca spike
#
rcbp_chirp_model --cellnum 576 --n_dbp1 1 --dbp1_am 2 --n_dbp2 2 --dbp2_am 2 --amarr 22 --make_cones 1 --cone_type 2 --vnoise 0 --rvcloc 0 --drm 30e3 --psyn1 823 --psyn3 807 --axdia 0.7 --spotdur 1.0 --spotdia 250 --g_dbp1_am 1e-12 --g_am_dbp1 4e-10 --dbp1_gtau 1 --g_dbp1_am2 1e-12 --g_am2_dbp1 2e-10 --g_dbp2_am 7e-10 --g_am_dbp2 3e-10 --am_sdur2 500 --am_sfall2 0 --am_atau 1 --am_sfall 2 --g_dbp2_am2 4e-10 --g_am2_dbp2 4e-12 --mxrot 0 --ninfo 2 --comnd_line_only 0 --one_run 1 --disp_c2 1 --dri 200 --cmult 0.002 --minten -0.039 --naab 0e-3 --ksoma 0e-3 --cadist 0e-3 --catdist 0.1e-3 --v 1 --d 0 --w 1.24 & 

#
# with 3 shorter flashes instead of 2 (2 depoarizations) in expt_cbp_chirp.cc, cmult 0.0012
#   makes no feedback because dbp2 has L-type channels.
#
rcbp_chirp_model --cellnum 576 --n_dbp1 1 --dbp1_am 2 --n_dbp2 2 --dbp2_am 2 --amarr 22 --make_cones 1 --cone_type 2 --vnoise 0 --rvcloc 0 --drm 30e3 --psyn1 823 --psyn3 807 --axdia 0.7 --spotdur 0.5 --spotdia 250 --g_dbp1_am 1e-12 --g_am_dbp1 4e-10 --dbp1_gtau 1 --g_dbp1_am2 1e-12 --g_am2_dbp1 2e-10 --g_dbp2_am 7e-10 --g_am_dbp2 3e-10 --am_sdur2 500 --am_sfall2 0 --am_atau 1 --am_sfall 2 --g_dbp2_am2 4e-10 --g_am2_dbp2 4e-12 --mxrot 0 --ninfo 2 --comnd_line_only 0 --one_run 1 --disp_c2 1 --dri 200 --cmult 0.0012 --minten -0.039 --naab 0e-3 --ksoma 0e-3 --cadist 0e-3 --catdist 0.1e-3 --v 1 --d 0 --w 1.24 & 

#
# without transient in cone->dbp1, (nval_cbp_chirp.n), 
#   makes initial Ca spike followed by feedback transient.
#   compared to T-type (catdist = 0, cadist = 1e-3), same cbp transient, bu with just feedback transient
#
rcbp_chirp_model --cellnum 576 --n_dbp1 1 --dbp1_am 2 --n_dbp2 2 --dbp2_am 2 --amarr 22 --make_cones 1 --cone_type 2 --vnoise 0 --rvcloc 0 --drm 30e3 --psyn1 823 --psyn3 807 --axdia 0.7 --spotdur 0.75 --spotdia 250 --g_dbp1_am 1e-12 --g_am_dbp1 4e-10 --dbp1_gtau 1 --g_dbp1_am2 1e-12 --g_am2_dbp1 2e-10 --g_dbp2_am 7e-10 --g_am_dbp2 3e-10 --am_sdur2 500 --am_sfall2 0 --am_atau 1 --am_sfall 2 --g_dbp2_am2 4e-10 --g_am2_dbp2 4e-12 --mxrot 0 --ninfo 2 --comnd_line_only 0 --one_run 1 --disp_c2 1 --dri 200 --cmult 0.002 --minten -0.041 --naab 0e-3 --ksoma 0e-3 --cadist 0e-3 --catdist 0.1e-3 --v 1 --d 0 --w 1.24 & 

