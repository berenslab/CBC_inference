#
# make 1 am with 11 dbp1s
#
retsim --expt cbp_chirp --cbptype dbp1 --cbptype2 dbp2 --n_dbp1 100 --n_dbp2 100 --ksoma 0 --nahd 0 --dbp1_nscale -2.09 --dbp2_nscale -2.09 --am_nscale -3.09 --tip1 807 --tip2 803 --presyn1 807 --presyn2 803 --remove_nconns 1 --ninfo 2 --info_chan 0 --amarr 11 --mxrot -0 --g_am_dbp1 5e-10 --spotdia 300 --n_dbp2 0 -d 1 -v | vid                  

#
# set dbp1_gtau, am_gtau
#
retsim --expt cbp_chirp --cbptype dbp1 --cbptype2 dbp2 --n_dbp1 100 --n_dbp2 100 --ksoma 0 --nahd 0 --dbp1_nscale -2.09 --dbp2_nscale -2.09 --am_nscale -3.09 --tip1 807 --tip2 803 --presyn1 807 --presyn2 803 --remove_nconns 1 --ninfo 2 --info_chan 0 --amarr 11 --mxrot -0 --g_am_dbp1 5e-10 --spotdia 300 --n_dbp2 0 --dbp1_gtau 100 --am_sdur 50 -v | vid & 

#
# try different combinations of dbp1_gtau and am_sdur
#
mosrun -l retsim --expt cbp_chirp --cbptype dbp1 --cbptype2 dbp2 --n_dbp1 100 --n_dbp2 100 --ksoma 0 --nahd 0 --dbp1_nscale -2.09 --dbp2_nscale -2.09 --am_nscale -3.09 --tip1 807 --tip2 803 --presyn1 807 --presyn2 803 --remove_nconns 1 --ninfo 2 --info_chan 0 --amarr 11 --mxrot -0 --g_am_dbp1 5e-10 --spotdia 300 --n_dbp2 0 --am_sdur 2 --dbp1_gtau 1 >& cbp_chirp.2.1.r &

mosrun -l retsim --expt cbp_chirp --cbptype dbp1 --cbptype2 dbp2 --n_dbp1 100 --n_dbp2 100 --ksoma 0 --nahd 0 --dbp1_nscale -2.09 --dbp2_nscale -2.09 --am_nscale -3.09 --tip1 807 --tip2 803 --presyn1 807 --presyn2 803 --remove_nconns 1 --ninfo 2 --info_chan 0 --amarr 11 --mxrot -0 --g_am_dbp1 5e-10 --spotdia 300 --n_dbp2 0 --am_sdur 2 --dbp1_gtau 1 --ampar 7 >& cbp_chirp.2.1n.r &

mosrun -l retsim --expt cbp_chirp --cbptype dbp1 --cbptype2 dbp2 --n_dbp1 100 --n_dbp2 100 --ksoma 0 --nahd 0 --dbp1_nscale -2.09 --dbp2_nscale -2.09 --am_nscale -3.09 --tip1 807 --tip2 803 --presyn1 807 --presyn2 803 --remove_nconns 1 --ninfo 2 --info_chan 0 --amarr 11 --mxrot -0 --g_am_dbp1 5e-10 --spotdia 300 --n_dbp2 0 --am_sdur 10 --dbp1_gtau 10 >& cbp_chirp.10.10.r &

