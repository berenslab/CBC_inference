#
# cadist 1.5e-3, gives bigger diff in rate
#
rcbp_chirp_model --cellnum 576 --n_dbp1 1 --dbp1_am 2 --n_dbp2 2 --dbp2_am 2 --amarr 22 --make_cones 1 --cone_type 2 --vnoise 0 --rvcloc 0 --drm 30e3 --psyn1 823 --psyn3 807 --axdia 0.7 --spotdur 1.0 --spotdia 250 --g_dbp1_am 1e-12 --g_am_dbp1 4e-10 --dbp1_gtau 1 --g_dbp1_am2 1e-12 --g_am2_dbp1 2e-10 --g_dbp2_am 7e-10 --g_am_dbp2 3e-10 --am_sdur2 500 --am_sfall2 0 --am_atau 1 --am_sfall 2 --g_dbp2_am2 4e-10 --g_am2_dbp2 4e-12 --mxrot 0 --ninfo 2 --comnd_line_only 0 --one_run 1 --disp_c2 1 --dri 200 --cmult 0.0012 --minten -0.041 --cadist 1.5e-3 --v 1 --d 0 --w 1.24

#
# when am_sdur is less than 500, oscillates
#
rcbp_chirp_model --cellnum 576 --n_dbp1 1 --dbp1_am 2 --n_dbp2 2 --dbp2_am 2 --amarr 22 --make_cones 1 --cone_type 2 --vnoise 0 --rvcloc 0 --drm 30e3 --psyn1 823 --psyn3 807 --axdia 0.7 --spotdur 1.0 --spotdia 250 --g_dbp1_am 1e-12 --g_am_dbp1 4e-10 --dbp1_gtau 1 --g_dbp1_am2 1e-12 --g_am2_dbp1 2e-10 --g_dbp2_am 7e-10 --g_am_dbp2 3e-10 --am_sdur2 50 --am_sfall2 0 --am_atau 1 --am_sfall 2 --g_dbp2_am2 4e-10 --g_am2_dbp2 4e-12 --mxrot 0 --ninfo 2 --comnd_line_only 0 --one_run 1 --disp_c2 1 --dri 200 --cmult 0.0012 --minten -0.041 --cadist 1e-3 --v 1 --d 0 --w 1.24

#
# less feedback am -> dbp2, damped oscillation 
#
rcbp_chirp_model --cellnum 576 --n_dbp1 1 --dbp1_am 2 --n_dbp2 2 --dbp2_am 2 --amarr 22 --make_cones 1 --cone_type 2 --vnoise 0 --rvcloc 0 --drm 30e3 --psyn1 823 --psyn3 807 --axdia 0.7 --spotdur 1.0 --spotdia 250 --g_dbp1_am 1e-12 --g_am_dbp1 4e-10 --dbp1_gtau 1 --g_dbp1_am2 1e-12 --g_am2_dbp1 2e-10 --g_dbp2_am 7e-10 --g_am_dbp2 1e-10 --am_sdur2 50 --am_sfall2 0 --am_atau 1 --am_sfall 2 --g_dbp2_am2 4e-10 --g_am2_dbp2 4e-12 --mxrot 0 --ninfo 2 --comnd_line_only 0 --one_run 1 --disp_c2 1 --dri 200 --cmult 0.0012 --minten -0.041 --cadist 1e-3 --v 1 --d 0 --w 1.24

# even less feedback, some instability, more dispersion in voltage, rate
#  similar amount of inhibition from am, am2 gives greater dispersion
#  value of am_sdur2 (am -> dbp2) sets duration of feedback peak
#
rcbp_chirp_model --cellnum 576 --n_dbp1 1 --dbp1_am 2 --n_dbp2 2 --dbp2_am 2 --amarr 22 --make_cones 1 --cone_type 2 --vnoise 0 --rvcloc 0 --drm 30e3 --psyn1 823 --psyn3 807 --axdia 0.7 --spotdur 1.0 --spotdia 250 --g_dbp1_am 1e-12 --g_am_dbp1 4e-10 --dbp1_gtau 1 --g_dbp1_am2 1e-12 --g_am2_dbp1 2e-10 --g_dbp2_am 7e-10 --g_am_dbp2 0.7e-10 --am_sdur2 50 --am_sfall2 0 --am_atau 1 --am_sfall 2 --g_dbp2_am2 4e-10 --g_am2_dbp2 4e-12 --mxrot 0 --ninfo 2 --comnd_line_only 0 --one_run 1 --disp_c2 1 --dri 200 --cmult 0.0012 --minten -0.041 --cadist 1e-3 --v 1 --d 0 --w 1.24

#
# make am2->dbp1 slower, nfilt = 1 (nval_cpb_chirp.n)
#
rcbp_chirp_model --cellnum 576 --n_dbp1 1 --dbp1_am 2 --n_dbp2 2 --dbp2_am 2 --amarr 22 --make_cones 1 --cone_type 2 --vnoise 0 --rvcloc 0 --drm 30e3 --psyn1 823 --psyn3 807 --axdia 0.7 --spotdur 1.0 --spotdia 250 --g_dbp1_am 1e-12 --g_am_dbp1 3e-10 --dbp1_gtau 1 --g_dbp1_am2 1e-12 --g_am2_dbp1 10e-10 --g_dbp2_am 7e-10 --g_am_dbp2 0.7e-10 --am_sdur2 50 --am_sfall2 0 --am_atau 1 --am_sfall 2 --am2_sdur 50 --g_dbp2_am2 4e-10 --g_am2_dbp2 4e-12 --mxrot 0 --ninfo 2 --comnd_line_only 0 --one_run 1 --disp_c2 1 --dri 200 --cmult 0.0012 --minten -0.041 --cadist 1e-3 --v 1 --d 0 --w 1.24

# block am->dbp1, leave delayed am2->dbp1, makes longer peak
#
rcbp_chirp_model --cellnum 576 --n_dbp1 1 --dbp1_am 2 --n_dbp2 2 --dbp2_am 2 --amarr 22 --make_cones 1 --cone_type 2 --vnoise 0 --rvcloc 0 --drm 30e3 --psyn1 823 --psyn3 807 --axdia 0.7 --spotdur 1.0 --spotdia 250 --g_dbp1_am 1e-12 --g_am_dbp1 3e-12 --dbp1_gtau 1 --g_dbp1_am2 1e-12 --g_am2_dbp1 10e-10 --g_dbp2_am 7e-10 --g_am_dbp2 0.7e-10 --am_sdur2 50 --am_sfall2 0 --am_atau 1 --am_sfall 2 --am2_sdur 50 --g_dbp2_am2 4e-10 --g_am2_dbp2 4e-12 --mxrot 0 --ninfo 2 --comnd_line_only 0 --one_run 1 --disp_c2 1 --dri 200 --cmult 0.0012 --minten -0.041 --cadist 1e-3 --v 1 --d 0 --w 1.24

