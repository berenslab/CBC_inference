# use presyn region 8 for dbp1, dbp2
#
retsim --expt cbp_chirp --cbptype dbp1 --cbptype2 dbp2 --ksoma 0 --nahd 0 --dbp1_nscale -3.09 --dbp2_nscale -3.09 --am_nscale -3.09 --tip1 807 --tip2 803 --presyn1 8 --presyn2 8 --remove_nconns 1 --ninfo 2 --info_chan 0 --amarr 21 --mxrot -0 --g_am_dbp1 5e-10 --spotdia 300 -r 11113 -d 1 -v | vid


#
# to look at dbp1 cell numbers:
#
retsim --expt cbp_chirp --cbptype dbp1 --cbptype2 dbp2 --n_dbp1 100 --n_dbp2 100 --ksoma 0 --nahd 0 --dbp1_nscale -2.09 --dbp2_nscale -2.09 --am_nscale -2.09 --tip1 807 --tip2 803 --presyn1 807 --presyn2 803 --remove_nconns 1 --ninfo 2 --info_chan 0 --amarr 21 --mxrot -0 --g_am_dbp1 5e-10 --spotdia 300 --n_dbp2 0 -d 10 -v | vid

