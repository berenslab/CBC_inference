# povray -h2000 -w2000 -ix
# ocular x.png

#
# display one cell
#
rcbp_chirp_model --cellnum 576 --n_dbp1 1 --mxrot -90 --v 1 --d 1

#
# display one cell with cone input
#
rcbp_chirp_model --cellnum 576 --n_dbp1 1 --mxrot -90 --make_cones 1 --ninfo 2 --d 1 --v 1 

# display, render with povray
#
rcbp_chirp_model --cellnum 576 --n_dbp1 1 --mxrot -90 --make_cones 1 --ninfo 2 --d 1 --R 1 


# display all cbps
#
rcbp_chirp_model --cellnum all --n_dbp1 1 --d 1 -R 1

#
# display with no recponts labeled as "Rx"
#
rcbp_chirp_model --cellnum 576 --n_dbp1 1 --v 1 --d 1 --no_recponts 1

# display one whole model 
#
rcbp_chirp_model --cellnum 576 --n_dbp1 11 --v 1 --d 1

# run model, command line only
#
rcbp_chirp_model --cellnum 576 --n_dbp1 100 --dbp1_nscale -3.10 --mxrot -90 --v 1 --comnd_line_only 1

# run model with cones as VC transducers
#
rcbp_chirp_model --cellnum 576 --n_dbp1 1 --make_cones 1 --cone_type 2 --cmult 0.002 --vnoise 0 --scontrast -1 --ninfo 2 --v 1 

#
# run model with cones as VC transducers, no vclamp in cbp
#
rcbp_chirp_model --cellnum 576 --n_dbp1 1 --make_cones 1 --cone_type 2 --cmult 0.002 --vnoise 0 --rvcloc 0 --scontrast -1 --ninfo 2 --v 1

# run model with cones, using light-driven cone transduction
#
rcbp_chirp_model --cellnum 576 --n_dbp1 1 --make_cones 1 --cmult 10000 --minten 10000 --vnoise 0 --rvcloc 0 --ninfo 2 --v 1 


# display whole model, make png file
# 
rcbp_chirp_model --cellnum 576 --n_dbp1 11 --v 0 --d 1 --R 1 --mxrot -90

#
#
#  run full model
#
rcbp_chirp_model --cellnum 576 --n_dbp1 11 --dbp1_nscale -3.10 --mxrot -90 --v 1 --comnd_line_only 0
#
#
# one cell, default vc and recording locations
#
rcbp_chirp_model --cellnum 576 --n_dbp1 1 --v 1 --comnd_line_only 0
#
rcbp_chirp_model --cellnum 576 --n_dbp1 11 --spotdia 300 --comnd_line_only 0
#
rcbp_chirp_model --cellnum 576 --n_dbp1 11 --spotdia 50  --comnd_line_only 0

#   Run with fewer combinations of axdia, recpnts
#
rcbp_chirp_model --cellnum 576 --n_dbp1 1 --pjobs 1 --naxdia 2 --nrecpnt 2

#
# one cell, set vc location
#
rcbp_chirp_model --cellnum 576 --n_dbp1 1 --v 1 --comnd_line_only 0 --vcloc 801
#
# one cell, set recording point
#
rcbp_chirp_model --cellnum 576 --n_dbp1 1 --v 1 --comnd_line_only 0 --rp1 802

#
#  set dia factor for bipolar cell
#
rcbp_chirp_model --cellnum 576 --n_dbp1 11 --axdia 0.8 --v 1 --comnd_line_only 0 &

#  set feedback conductance
#
rcbp_chirp_model --cellnum 576 --n_dbp1 11 --g_am_dbp1 10e-10 --v 1 --comnd_line_only 0 &

#
# set cbp feedforward synaptic conductance
#
rcbp_chirp_model --cellnum 576 --n_dbp1 11 --g_dbp1 20e-10 --v 1 --comnd_line_only 0 &

#
#  set feedback tau of GABA channel in dbp1 (see chanparams_cbp_chirp)
#
rcbp_chirp_model --cellnum 576 --n_dbp1 11 --dbp1_gtau 15 --v 1 --comnd_line_only 0 &

#
# set feedforward tau for AMPA channel in am (see chanparams_cbp_chirp)
#
rcbp_chirp_model --cellnum 576 --n_dbp1 11 --am_atau 15 --v 1 --comnd_line_only 0 &

#
#  set feedback tau of am->dbp1 synapse (see nval_cbp_chirp.n)
#
rcbp_chirp_model --cellnum 576 --n_dbp1 11 --am_sdur 20 --v 1 --comnd_line_only 0 &


# set fast GABA feedback, with vesicle noise
#
rcbp_chirp_model --cellnum 576 --n_dbp1 11 --g_dbp1 10e-10 --vnoise 1 --dbp1_gtau 1 --am_sdur 2 --am_atau 1 --v 1 --comnd_line_only 0

#
# set fast feedback, no vesicle noise
#
rcbp_chirp_model --cellnum 576 --n_dbp1 11 --g_dbp1 10e-10 --vnoise 0 --dbp1_gtau 1 --am_sdur 2 --am_atau 1 --v 1 --comnd_line_only 0

#
#  Run one dbp1 changing axdia, recpnts.
#    To run on all cells, replace 576 with "all"
# 
#    To run jobs sequentially:
# 
rcbp_chirp_model --cellnum 576 --n_dbp1 1 

# rcbp_chirp_model --cellnum 576 --n_dbp1 1 --mosrun "mosrun -l"

#
#    Run in parallel with multiple cores and smp kernel, add "--pjobs 1".
#    Then after the jobs are all finished, run with "--plotpdf 1" to make pdf files.
#
rcbp_chirp_model --cellnum 576 --n_dbp1 1 --pjobs 1
rcbp_chirp_model --cellnum 576 --n_dbp1 1 --plotpdf 1

