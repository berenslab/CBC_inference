#
#
# test oscillations
#
# 7,7,7 gives 4 osc, strong
#
rcbp_chirp_model --cellnum 576 --n_dbp1 11 --g_dbp1 10e-10 --vnoise 0 --dbp1_gtau 7 --am_sdur 7 --am_atau 7 --v 1 --comnd_line_only 0 --one_run 1

#
# 7,20,7 gives 3 osc, strong
#
rcbp_chirp_model --cellnum 576 --n_dbp1 11 --g_dbp1 10e-10 --vnoise 0 --dbp1_gtau 7 --am_sdur 20 --am_atau 7 --v 1 --comnd_line_only 0 --one_run 1

#
# 20,20,20 gives 2.5 osc, strong
#
rcbp_chirp_model --cellnum 576 --n_dbp1 11 --g_dbp1 10e-10 --vnoise 0 --dbp1_gtau 20 --am_sdur 20 --am_atau 20 --v 1 --comnd_line_only 0 --one_run 1

#
# 5,5,5 gives 4.5 osc, weaker
#
rcbp_chirp_model --cellnum 576 --n_dbp1 11 --g_dbp1 10e-10 --vnoise 0 --dbp1_gtau 5 --am_sdur 5 --am_atau 5 --v 1 --comnd_line_only 0 --one_run 1

# 2,5,2 gives 1.5 osc, weak
#
rcbp_chirp_model --cellnum 576 --n_dbp1 11 --g_dbp1 10e-10 --vnoise 0 --dbp1_gtau 2 --am_sdur 5 --am_atau 2 --v 1 --comnd_line_only 0 --one_run 1

# 1,1,1 with g_dbp1=20e-10 gives short transient
#
 rcbp_chirp_model --cellnum 576 --n_dbp1 11 --g_dbp1 20e-10 --vnoise 0 --dbp1_gtau 1 --am_sdur 1 --am_atau 1 --v 1 --comnd_line_only 0 --one_run 1

# 30,1,30 gives one transient in am, 2 peaks in cbp
# 30,30,30 gives one transient in am, 2 peaks in cbp
#
 rcbp_chirp_model --cellnum 576 --n_dbp1 11 --g_dbp1 10e-10 --vnoise 0 --dbp1_gtau 30 --am_sdur 1 --am_atau 30 --v 1 --comnd_line_only 0 --one_run 1

# 1,1,1 g_dbp1=30e-10 gives one very short transient
#
rcbp_chirp_model --cellnum 576 --n_dbp1 11 --g_dbp1 30e-10 --vnoise 0 --dbp1_gtau 1 --am_sdur 1 --am_atau 1 --cbplam2 0.2 --v 1 --comnd_line_only 0 --one_run 1

#  need to set up file name to include parameters that will be actively varied
#
#  light-driven cone, stable feedback
#
 rcbp_chirp_model --cellnum 576 --n_dbp1 11 --make_cones 1 --cmult 10000 --minten 10000 --vnoise 0 --rvcloc 0 --drm 30e3 --g_am_dbp1 4e-10 --ninfo 2 --v 1 --mxrot 0 --comnd_line_only 0 

#  VC-driven cone, stable feedback
#
 rcbp_chirp_model --cellnum 576 --n_dbp1 11 --make_cones 1 --cone_type 2 --cmult 0.002 --vnoise 0 --rvcloc 0 --drm 30e3 --g_am_dbp1 4e-10 --scontrast -1 --ninfo 2 --v 1 --mxrot 0 --comnd_line_only 0 

#
# VC-driven cone, thinner axdia
#
 rcbp_chirp_model --cellnum 576 --n_dbp1 11 --make_cones 1 --cone_type 2 --cmult 0.002 --vnoise 0 --rvcloc 0 --drm 30e3 --g_am_dbp1 4e-10 --axdia 0.8 --scontrast -1 --ninfo 2 --v 1 --mxrot 0 --comnd_line_only 0 

rcbp_chirp_model --cellnum 576 --n_dbp1 11 --n_dbp2 11 --amarr 22 --make_cones 1 --cone_type 2 --cmult 0.002 --vnoise 0 --rvcloc 0 --drm 30e3 --g_am_dbp1 4e-10 --psyn2 801 --axdia 0.8 --scontrast -1 --ninfo 2 --v 1 --mxrot 0 --comnd_line_only 0

