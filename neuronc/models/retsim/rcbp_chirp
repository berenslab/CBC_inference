
# display cell, stimulus point, recording points
#
retsim --expt cbp_chirp --ninfo 2 --stimloc 101 --recpnt1 501 --recpnt2 502 --node_scale -3.2 --mxrot -90 -d 1 -v | vid

# run chirp with stimulus, recording points
#
retsim --expt cbp_chirp --ninfo 2 --stimloc 101 --recpnt1 501 --recpnt2 502 --mxrot -90 -v | vid

#
# just chirp and vcontrast, no spot
#  original version
#
retsim --expt cbp_chirp --ninfo 2 --stimloc 101 --recpnt1 501 --recpnt2 502 --mxrot -90 --spotdur 0 --prestimdur 0.1 --stimtime 0.05 --fstart 1 --fincr 0.05 --stimdur 1 --cmult 0.02 -v | vid

#
# with ft^2
#  original length, no spot
#
retsim --expt cbp_chirp --ninfo 2 --stimloc 101 --recpnt1 501 --recpnt2 502 --mxrot -90 --spotdur 0 --prestimdur 0.1 --stimtime 0.05 --fstart 2 --fincr 0.05 --stimdur 1 --cmult 0.02 -v | vid

# with spot
# 
retsim --expt cbp_chirp --ninfo 2 --stimloc 101 --recpnt1 501 --recpnt2 502 --mxrot -90 --predur 0.5 --spotdur 0.5 --prestimdur 0.5 --stimtime 0.1 --fstart 1 --fincr 0.02 --stimdur 2 --cmult 0.02 --minten -0.038 -v | vid 

#
# gives spikes, minten -45, cmult 0.025
#
retsim --expt cbp_chirp --ninfo 2 --stimloc 101 --recpnt1 501 --recpnt2 502 --mxrot -90 --predur 0.5 --spotdur 0.5 --prestimdur 0.5 --stimtime 0.1 --fstart 1 --fincr 0.005 --stimdur 2 --cmult 0.025 --minten -0.045 -v | vid 

# gives spikes, minten -45, cmult 0.03
#
retsim --expt cbp_chirp --ninfo 2 --stimloc 101 --recpnt1 501 --recpnt2 502 --mxrot -90 --predur 0.5 --spotdur 0.5 --prestimdur 0.5 --stimtime 0.1 --fstart 1 --fincr 0.005 --stimdur 2 --cmult 0.030 --minten -0.045 -v | vid -c > cpb_chirp-0.30.0.45.ps

retsim --expt cbp_chirp --ninfo 2 --stimloc 101 --recpnt1 501 --recpnt2 502 --mxrot -90 --predur 0.5 --spotdur 0.5 --prestimdur 0.5 --stimtime 0.1 --fstart 1 --fincr 0.005 --stimdur 2 --cmult 0.030 --minten -0.05 -v | vid -c > cpb_chirp-0.30.0.45.ps

