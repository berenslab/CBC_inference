#
# 2 cell types, no reciprocal feedback from dbp1, but reciprocal and lateral feedback from dbp2
#   this makes slight transient in amacrine, very pronounced transient in dbp1
#
rcbp_chirp_model --cellnum 576 --n_dbp1 1 --dbp1_am 2 --n_dbp2 1 --dbp2_am 2 --amarr 11 --make_cones 0 --vnoise 0 --cmult 0.015 --rvcloc 0 --drm 30e3 --psyn1 801 --psyn2 801 --axdia 1.0 --spotdur 1.0 --spotdia 120 --g_dbp1_am 0.1e-10 --g_am_dbp1 4e-10 --dbp1_gtau 2 --g_dbp2_am 4e-10 --g_am_dbp2 2e-10 --am_sdur 1 --am_atau 30 --am_sfall 200 --mxrot 0 --ninfo 2 --v 1 --d 0 --comnd_line_only 0

#
# same, but stronger feedback to dbp2, makes oscillations
#
rcbp_chirp_model --cellnum 576 --n_dbp1 1 --dbp1_am 2 --n_dbp2 1 --dbp2_am 2 --amarr 11 --make_cones 0 --vnoise 0 --cmult 0.015 --rvcloc 0 --drm 30e3 --psyn1 801 --psyn2 801 --axdia 1.0 --spotdur 1.0 --spotdia 120 --g_dbp1_am 0.1e-10 --g_am_dbp1 4e-10 --dbp1_gtau 2 --g_dbp2_am 4e-10 --g_am_dbp2 4e-10 --am_sdur 1 --am_atau 30 --am_sfall 200 --mxrot 0 --ninfo 2 --v 1 --d 0 --comnd_line_only 0

#
#  middle strength feedback, g_am_dbp2 3e-10, no oscillations
#
rcbp_chirp_model --cellnum 576 --n_dbp1 1 --dbp1_am 2 --n_dbp2 1 --dbp2_am 2 --amarr 11 --make_cones 0 --vnoise 0 --cmult 0.015 --rvcloc 0 --drm 30e3 --psyn1 801 --psyn2 801 --axdia 1.0 --spotdur 1.0 --spotdia 120 --g_dbp1_am 0.1e-10 --g_am_dbp1 4e-10 --dbp1_gtau 2 --g_dbp2_am 4e-10 --g_am_dbp2 3e-10 --am_sdur 1 --am_atau 30 --am_sfall 200 --mxrot 0 --ninfo 2 --v 1 --d 0 --comnd_line_only 0

# use cmult=0.018 for axdia = 0.8
#
rcbp_chirp_model --cellnum 576 --n_dbp1 1 --dbp1_am 2 --n_dbp2 1 --dbp2_am 2 --amarr 11 --make_cones 0 --vnoise 0 --cmult 0.018 --rvcloc 0 --drm 30e3 --psyn1 801 --psyn2 801 --axdia 0.8 --spotdur 1.0 --spotdia 120 --g_dbp1_am 1e-12 --g_am_dbp1 4e-10 --dbp1_gtau 2 --g_dbp2_am 4e-10 --g_am_dbp2 0.5e-10 --am_sdur 1 --am_atau 30 --am_sfall 200 --mxrot 0 --ninfo 2 --v 1 --d 0 --comnd_line_only 0 --one_run 1

# axdia 1.0 with small amount of feedback to dbp2
#
 rcbp_chirp_model --cellnum 576 --n_dbp1 1 --dbp1_am 2 --n_dbp2 1 --dbp2_am 2 --amarr 11 --make_cones 0 --vnoise 0 --cmult 0.015 --rvcloc 0 --drm 30e3 --psyn1 801 --psyn2 801 --axdia 1.0 --spotdur 1.0 --spotdia 120 --g_dbp1_am 1e-12 --g_am_dbp1 4e-10 --dbp1_gtau 2 --g_dbp2_am 4e-10 --g_am_dbp2 2e-10 --am_sdur 1 --am_atau 30 --am_sfall 200 --mxrot 0 --ninfo 2 --v 1 --d 0 --comnd_line_only 0 --one_run 1 

# set up current clamp transducer: itransducer, too much feedback, oscillates
#
rcbp_chirp_model --cellnum 576 --n_dbp1 1 --dbp1_am 2 --n_dbp2 1 --dbp2_am 2 --amarr 11 --make_cones 0 --vnoise 0 --cmult 0.015 --rvcloc 0 --drm 30e3 --psyn1 801 --psyn2 801 --axdia 1.0 --spotdur 1.0 --spotdia 120 --g_dbp1_am 0.1e-10 --g_am_dbp1 4e-10 --dbp1_gtau 2 --g_dbp2_am 4e-10 --g_am_dbp2 3e-10 --am_sdur 1 --am_atau 1 --am_sfall 20 --mxrot 0 --ninfo 2 --v 1 --d 0 --comnd_line_only 0 --one_run 1 --B 7 --minten 8e-12 --cmult 4e-12 --setxmin -0.5

#
# less feedback, doesn't oscillate
#
rcbp_chirp_model --cellnum 576 --n_dbp1 1 --dbp1_am 2 --n_dbp2 1 --dbp2_am 2 --amarr 11 --make_cones 0 --vnoise 0 --cmult 0.015 --rvcloc 0 --drm 30e3 --psyn1 801 --psyn2 801 --axdia 1.0 --spotdur 1.0 --spotdia 120 --g_dbp1_am 0.1e-10 --g_am_dbp1 4e-10 --dbp1_gtau 1 --g_dbp2_am 4e-10 --g_am_dbp2 1.5e-10 --am_sdur 1 --am_atau 1 --am_sfall 10 --mxrot 0 --ninfo 2 --v 1 --d 0 --comnd_line_only 0 --one_run 1 --B 7 --minten 8.4e-12 --cmult 8e-12

