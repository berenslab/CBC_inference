#
# a little less lateral inhibition, scale window
#   gives 7 mV diff between presyn1 and other recording points
#
rcbp_chirp_model --cellnum 576 --n_dbp1 1 --dbp1_am 2 --n_dbp2 1 --dbp2_am 2 --amarr 11 --make_cones 0 --vnoise 0 --rvcloc 0 --drm 30e3 --psyn1 823 --psyn2 801 --axdia 0.7 --spotdur 1.0 --spotdia 120 --g_dbp1_am 1e-12 --g_am_dbp1 4e-10 --dbp1_gtau 1 --g_dbp2_am 8e-10 --g_am_dbp2 30e-12 --am_sdur 1 --am_atau 1 --am_sfall 2 --mxrot 0 --ninfo 2 --v 0 --d 0 --comnd_line_only 0 --one_run 1 --disp_c2 0 --dri 200 --cmult 2.8e-12 --minten 9e-12 --w 1.4

#
# vclamp in cones
#
 rcbp_chirp_model --cellnum 576 --n_dbp1 1 --dbp1_am 2 --n_dbp2 1 --dbp2_am 2 --amarr 11 -make_cones 1 --cone_type 2 --vnoise 0 --rvcloc 0 --drm 30e3 --psyn1 823 --psyn2 801 --axdia 0.7 --spotdur 1.0 --spotdia 120 --g_dbp1_am 1e-12 --g_am_dbp1 4e-10 --dbp1_gtau 1 --g_dbp2_am 8e-10 --g_am_dbp2 30e-12 --am_sdur 1 --am_atau 1 --am_sfall 2 --mxrot 0 --ninfo 2 --v 1 --d 0 --comnd_line_only 0 --one_run 1 --disp_c2 0 --w 1.4 --cmult 0.002 --minten -0.040

