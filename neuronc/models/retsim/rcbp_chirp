retsim --expt cbp_chirp --dbp1_file morph_cbp_0572_t5r --make_dbp1 1 --n_dbp1 1 --ninfo 2 --plotlabel cbp_0572_t5r --predur 0.5 --spotdur 0.5 --prestimdur 0.5 --stimtime 0.1 --fstart 1 --fincr 0.005 --stimdur 2 --cmult 0.030 --minten -0.05 --mxrot -90 --tip1 1437 --tip2 1409 --tip3 1280 -d 1 -v | vid -w 1.5

retsim --expt cbp_chirp --dbp1_file morph_cbp_0572_t5r --dbp1_densfile dens_cbp_chirp.n --make_dbp1 1 --n_dbp1 1 --ninfo 2 --plotlabel cbp_0572_t5r --predur 0.5 --spotdur 0.5 --prestimdur 0.5 --stimtime 0.1 --fstart 1 --fincr 0.005 --stimdur 2 --cmult 0.030 --minten -0.05 --recpnt1 1437 --recpnt2 1409 --recpnt3 1164 --recpnt4 1396 --recpnt5 1673 --recpnt6 1095 --recpnt7 627 --mxrot 0 -d 1 -v | vid -w 1.5 

retsim --expt cbp_chirp --dbp1_file morph_cbp_0572_t5r --dbp1_densfile dens_cbp_chirp.n --make_dbp1 1 --n_dbp1 1 --ninfo 2 --plotlabel cbp_0572_t5r --predur 0.5 --spotdur 0.5 --prestimdur 0.5 --stimtime 0.1 --fstart 1 --fincr 0.005 --stimdur 2 --cmult 0.030 --minten -0.05 --tip1 801 --tip2 802 --tip3 803 --tip4 804 --tip5 805 --tip6 806 --tip7 807 --tip8 808 --tip9 809 --mxrot 0 -d 1 -v | vid -w 1.5

retsim --expt cbp_chirp --dbp1_file morph_cbp_0581_t5r --dbp1_densfile dens_cbp_chirp.n --make_dbp1 1 --n_dbp1 1 --ninfo 2 --plotlabel cbp_0581_t5r --predur 0.5 --spotdur 0.5 --prestimdur 0.5 --stimtime 0.1 --fstart 1 --fincr 0.005 --stimdur 2 --cmult 0.030 --minten -0.05 --tip1 801 --tip2 802 --tip3 803 --tip4 804 --tip5 805 --tip6 806 --tip7 807 --tip8 808 --tip9 809 --tip10 810 --tip11 811 --tip12 812 --mxrot 0 -d 1 -v | vid -w 1.5

