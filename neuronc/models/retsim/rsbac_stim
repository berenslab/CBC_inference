To print out synapse radial distances:

sbspac = 145, set_synspac 7, sbac_isynanpo 60

retsim --barvel 1 --expt sbac_stim --sbac_file morph_sbac_168c --sbac_densfile dens_sbacc.n --sbac_densfile2 dens_sbacd.n --sb_rm 1e4 --sbarr 107 --sbspac 145 --set_synspac 7 --sb_vs -0.049 --sb_vr -0.060 --radincr 20 --soma_z -50 --sbac_thetax 4 --sbac_thetay 4 --predur 0.15 --vhold -0.06 --minten -0.038 --scontrast 0.0050 --outward 0 --iscal 2e-9 --dri 75 --ninfo 2 --set_vclamp 0 --sdia 0.3 --spdia 0.6 --dSomaDia 10 --sbac_dend1 1 --sbac_dend_cplam 0.1 --stimtype 1 --barwidth 30 --barlength 070 --velocity 00100 --stimdur 0.5 --nadist 0.003 --namid 0.003 --kdist 2e-3 --kmid 2e-3 --kprox 2e-3 --ksoma 3e-3 --cadist 0.001 --camid 0.001 --plotlabel sb7_midbp.360.0.0050.0.135.070.090.0.30.00100 -r 12345 --n_hpb1 0 --dbp1_cond 2.3e-10 --dbp1_nscale -2.1 --sbac_nscale -3.05 --sbaclm 0.1 --no_excit 1 --set_inhib 1.6e-10 --no_inhib 0 --sbac_isynanpo 60 --sbac_isynrngi -1360 --sbac_isynanni 80 --dbp1_anpi 20 --dbp1_anpo 090 --sbac_ithr -0.051 --stim_theta 0 --mxrot 0 --make_movie 0 --space_time 1 --frame_int 0.0002 --Vmin -0.06 --n_dbp1 0 --Vmax -0.045 --syn_rad_dist 1 --sbac_maxsdist 20 >& sbac7_sbspac_145 &

sbspac 200, set_synspac 10, sbac_isynanpo 120, maxsdist 10

retsim --barvel 1 --expt sbac_stim --sbac_file morph_sbac_168c --sbac_densfile dens_sbacc.n --sbac_densfile2 dens_sbacd.n --sb_rm 1e4 --sbarr 107 --sbspac 200 --set_synspac 10 --sb_vs -0.049 --sb_vr -0.060 --radincr 20 --soma_z -50 --sbac_thetax 4 --sbac_thetay 4 --predur 0.15 --vhold -0.06 --minten -0.038 --scontrast 0.0050 --outward 0 --iscal 2e-9 --dri 75 --ninfo 2 --set_vclamp 0 --sdia 0.3 --spdia 0.6 --dSomaDia 10 --sbac_dend1 1 --sbac_dend_cplam 0.1 --stimtype 1 --barwidth 30 --barlength 070 --velocity 00100 --stimdur 0.5 --nadist 0.003 --namid 0.003 --kdist 2e-3 --kmid 2e-3 --kprox 2e-3 --ksoma 3e-3 --cadist 0.001 --camid 0.001 --plotlabel sb7_midbp.360.0.0050.0.135.070.090.0.30.00100 -r 12345 --n_hpb1 0 --dbp1_cond 2.3e-10 --dbp1_nscale -2.1 --sbac_nscale -3.05 --sbaclm 0.1 --no_excit 1 --set_inhib 1.6e-10 --no_inhib 0 --sbac_isynanpo 120 --sbac_isynrngi -1360 --sbac_isynanni 80 --dbp1_anpi 20 --dbp1_anpo 090 --sbac_ithr -0.051 --stim_theta 0 --mxrot 0 --make_movie 0 --space_time 1 --frame_int 0.0002 --Vmin -0.06 --n_dbp1 0 --Vmax -0.045 --syn_rad_dist 1 --sbac_maxsdist 10 >& sbac7_sbspac_200 &

sbspac 250, set_synspac 5, sbac_isynanpo 150

retsim --barvel 1 --expt sbac_stim --sbac_file morph_sbac_168c --sbac_densfile dens_sbacc.n --sbac_densfile2 dens_sbacd.n --sb_rm 1e4 --sbarr 107 --sbspac 250 --set_synspac 5 --sb_vs -0.049 --sb_vr -0.060 --radincr 20 --soma_z -50 --sbac_thetax 4 --sbac_thetay 4 --predur 0.15 --vhold -0.06 --minten -0.038 --scontrast 0.0050 --outward 0 --iscal 2e-9 --dri 75 --ninfo 2 --set_vclamp 0 --sdia 0.3 --spdia 0.6 --dSomaDia 10 --sbac_dend1 1 --sbac_dend_cplam 0.1 --stimtype 1 --barwidth 30 --barlength 070 --velocity 00100 --stimdur 0.5 --nadist 0.003 --namid 0.003 --kdist 2e-3 --kmid 2e-3 --kprox 2e-3 --ksoma 3e-3 --cadist 0.001 --camid 0.001 --plotlabel sb7_midbp.360.0.0050.0.135.070.090.0.30.00100 -r 12345 --n_hpb1 0 --dbp1_cond 2.3e-10 --dbp1_nscale -2.1 --sbac_nscale -3.05 --sbaclm 0.1 --no_excit 1 --set_inhib 1.6e-10 --no_inhib 0 --sbac_isynanpo 150 --sbac_isynrngi -1360 --sbac_isynanni 80 --dbp1_anpi 20 --dbp1_anpo 090 --sbac_ithr -0.051 --stim_theta 0 --mxrot 0 --make_movie 0 --space_time 1 --frame_int 0.0002 --Vmin -0.06 --n_dbp1 0 --Vmax -0.045 --syn_rad_dist 1 --sbac_maxsdist 20 >& sbac7_sbspac_250 &

grep radial_dist sbac7_sbspac_145 | column 3 > sbac_rad_dist_145
grep radial_dist sbac7_sbspac_200 | column 3 > sbac_rad_dist_200
grep radial_dist sbac7_sbspac_250 | column 3 > sbac_rad_dist_250

bins -h -w 10 sbac_rad_dist_145 | plotmod | vid

bins -h -w 10 sbac_rad_dist_145 > sbac_rad_dist_145.txt
bins -h -w 10 sbac_rad_dist_200 > sbac_rad_dist_200.txt
bins -h -w 10 sbac_rad_dist_250 > sbac_rad_dist_250.txt


