# display voltage along dendrite only, Na, K chans, bipolars in annulus
#
retsim --expt sbac_stim --sbac_file morph_k0725_SAC --sbac_densfile dens_sbacc.n --sb_rm 1e4 --sbarr 0 --sb_vs -0.06 --sb_vr -0.060 --predur 0.05 --vhold -0.06 --minten -0.044 --scontrast 0.005 --outward 0 --iscal 2e-9 --mxrot -0 --dri 200 --ninfo 2 --set_vclamp 0 --sdia 0.3 --sbac_dend1 1 --sbac_dend_cplam 0.1 --stimtype 1 --barwidth 30 --barlength 500 --velocity 2000 --no_inhib 1 --stimdur 0.5 --nadist 10e-3 --namid 5e-3 --kdist 1e-3 --kmid 2e-3 --kprox 2e-3 --ksoma 2e-3 --cadist 0 --plotlabel sbac_nak_midbp -r 12345 --sbac_nscale -1.01 --n_hpb1 0 --dbp1_nscale -2.1 --sbac_nscale -3.01 --sbaclm 0.1 --dbp1_anpi 20 --dbp1_anpo 120 --sbac_thetax -3 --stim_theta 50 --mxrot 0 --make_movie 0 --space_time 1 --frame_int 0.0002 --Vmin -0.06 --Vmax -0.045 -v | vid -c > sbac_nak_midbp.ps &
#
