retsim --sineann 2 --expt sbac_stim --sbac_file morph_sbac_168c --sbac_densfile dens_sbacc.n --sbac_densfile2 dens_sbacd.n --sb_rm 20000 --sbarr 107 --sbspac 250 --set_synspac 5 --sb1mul 0.5 --sb_vs -0.049 --sb_vr -0.060 --radincr 20 --soma_z -50 --sbac_thetax 2 --sbac_thetay -2 --predur 0 --vhold -0.045 --minten -0.034 --scontrast 0.0950 --outward 0 --iscal 2e-9 --dri 50 --ninfo 2 --set_vclamp 0 --sdia 0.30 --spdia 0.6 --dSomaDia 10 --sbac_dend1 1 --sbac_dend_cplam 0.1 --stimtype 3 --ncycles 6 --barwidth 200 --barlength 70 --velocity 1000 --waveshape 0 --annrad 1000 --makenv 0 --stimdur 0.5 --nadist 0 --namid 0 --kdist 2e-3 --kmid 2e-3 --kprox 2e-3 --ksoma 3e-3 --cadist 0.001 --camid 0.001 --plotlabel sbac7-cclamp-0.0950-160-200-250-3-1.0-0.0-150-0.3-50-20000-45-1000.r -r 12346 --n_hpb1 0 --dbp1_cond 1.2e-10 --dbp1_nscale -2.1 --sbac_nscale -3.05 --sbaclm 0.1 --no_excit 1 --set_inhib 160e-12 --no_inhib 0 --wac_g 0 --sbac_isynanpo 150 --sbac_isynrngi -1360 --sbac_isynanni 80 --dbp1_anpi 20 --dbp1_anpo 150 --sbac_ithr -0.051 --stim_theta 0 --mxrot 0 --make_movie 0 --space_time 1 --frame_int 0.0002 --Vmin -0.06 --Vmax -0.045 -d 16 -v | vid

# check y label charsize

retsim --sineann 3 --expt sbac_stim --sbac_file morph_sbac_168c --sbac_densfile dens_sbacc.n --sbac_densfile2 dens_sbacd.n --sb_rm 20000 --sbarr 107 --sbspac 250 --set_synspac 5 --sb1mul 0.5 --sb_vs -0.049 --sb_vr -0.060 --radincr 20 --soma_z -50 --sbac_thetax 2 --sbac_thetay -2 --predur 0 --vhold -0.045 --minten -0.034 --scontrast 0.0950 --outward 0 --iscal 2e-9 --dri 50 --ninfo 2 --set_vclamp 1 --sdia 0.30 --spdia 0.6 --dSomaDia 10 --sbac_dend1 1 --sbac_dend_cplam 0.1 --stimtype 3 --ncycles 6 --barwidth 200 --barlength 70 --velocity 1000 --waveshape 0 --annrad 1000 --makenv 0 --stimdur 0.5 --nadist 0 --namid 0 --kdist 2e-3 --kmid 2e-3 --kprox 2e-3 --ksoma 3e-3 --cadist 0.001 --camid 0.001 --plotlabel sbac7-cclamp-0.0950-160-200-250-3-1.0-0.0-150-0.3-50-20000-45-1000.r -r 12346 --n_hpb1 0 --dbp1_cond 1.2e-10 --dbp1_nscale -2.1 --sbac_nscale -3.05 --sbaclm 0.1 --no_excit 1 --set_inhib 160e-12 --no_inhib 0 --wac_g 0 --sbac_isynanpo 150 --sbac_isynrngi -1360 --sbac_isynanni 80 --dbp1_anpi 20 --dbp1_anpo 150 --sbac_ithr -0.051 --stim_theta 0 --mxrot 0 --make_movie 0 --space_time 1 --frame_int 0.0002 --Vmin -0.06 --Vmax -0.045 -v | vid


