retsim --expt sbac_stim --sbac_file morph_sbac_168 --sbac_densfile dens_sbacc.n --sb_rm 1e4 --sbarr 103 --sbspac 70 --sb_vs -0.06 --sb_vr -0.060 --radincr 10 --soma_z -50 --sbac_thetax 4 --sbac_thetay 4 --predur 0.05 --vhold -0.06 --minten -0.044 --scontrast 0.005 --outward 0 --iscal 2e-9 --mxrot -0 --dri k00 --ninfo 2 --set_vclamp 0 --sdia 0.3 --dSomaDia 10 --sbac_dend1 1 --sbac_dend_cplam 0.1 --stimtype 1 --barwidth 30 --barlength 500 --velocity 2000 --stimdur 0.5 --nadist 10e-3 --namid 5e-3 --kdist 1e-3 --kmid 2e-3 --kprox 2e-3 --ksoma 2e-3 --cadist 0 --plotlabel sbc5 -r 12345 --n_hpb1 0 --dbp1_nscale -2.1 --sbac_nscale -3.05 --sbaclm 0.1 --no_excit 1 --no_inhib 0 --sbac_isynanpo 40 --sbac_isynrngi -1120 --sbac_isynanni 55 --dbp1_anpi 20 --dbp1_anpo 60 --sbac_ithr -0.05 --stim_theta 0 --mxrot 0 --make_movie 0 --space_time 1 --frame_int 0.0002 --Vmin -0.06 --Vmax -0.045 --n_dbp1 0 -d 1 -v | vid -c > sbac5_120.ps
