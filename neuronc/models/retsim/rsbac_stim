#    edit sbac_dsi to make file names same length
  sbac_dsi_printv_300 sbac_dsi.300.115.txt > sbac_dsi.300.115_v.txt
  sbac_dsi_printv_300 sbac_dsi.300.135.txt > sbac_dsi.300.135_v.txt
  sbac_dsi_printv_300 sbac_dsi.300.155.txt > sbac_dsi.300.155_v.txt
  sbac_dsi_printc_300 sbac_dsi.300.115.txt > sbac_dsi.300.115_c.txt
  sbac_dsi_printc_300 sbac_dsi.300.135.txt > sbac_dsi.300.135_c.txt
  sbac_dsi_printc_300 sbac_dsi.300.155.txt > sbac_dsi.300.155_c.txt
#    merge every 2 lines in sbac_dsi.135_v.txt, sbac_dsi.135.300_c.txt
#
  printdsi sbac7_midbp_ca.360.0.*.115.[01].r > sbac_dsi.360.115.txt
  printdsi sbac7_midbp_ca.360.0.*.135.[01].r > sbac_dsi.360.135.txt
  printdsi sbac7_midbp_ca.360.0.*.155.[01].r > sbac_dsi.360.155.txt
