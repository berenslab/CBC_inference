test space clamp

retsim --expt sbac_stim --sbac_file morph_sbac_168c --sbac_densfile dens_sbacc.n --sbac_densfile2 dens_sbacd.n --sb_rm 1e4 --sb_vs -0.049 --sb_vr -0.060 --sbarr 0 --sbac_isynanni 80 --dbp1_anpi 20 --dbp1_anpo 90 --wac_g 0 --dSomaDia 10 --sdia 0.27 --run_vclamp 1 -v | vid

mosrun -l -b -g retsim --expt sbac_stim --sbac_file morph_sbac_168c --sbac_densfile dens_sbacc.n --sbac_densfile2 dens_sbacd.n --sb_rm 1e4 --sb_vs -0.049 --sb_vr -0.060 --sbarr 0 --sbac_isynanni 80 --dbp1_anpi 20 --dbp1_anpo 90 --wac_g 0 --dSomaDia 10 --nadist 0 --namid 0 --cadist 0 --ksoma 0 --kprox 0 --kmid 0 --kdist 0 --sdia 0.27 --spdia 0.5 --run_vclamp 1 >& sbac_spaceclamp.0.27.0.5.r & 

mosrun -l -b -g retsim --expt sbac_stim --sbac_file morph_sbac_168c --sbac_densfile dens_sbacc.n --sbac_densfile2 dens_sbacd.n --sb_rm 1e4 --sb_vs -0.049 --sb_vr -0.060 --sbarr 0 --sbac_isynanni 80 --dbp1_anpi 20 --dbp1_anpo 90 --wac_g 0 --dSomaDia 10 --nadist 0 --namid 0 --cadist 0 --ksoma 0 --kprox 0 --kmid 0 --kdist 0 --sdia 0.3 --spdia 0.5 --run_vclamp 1 >& sbac_spaceclamp.0.3.0.5.r & 

#  2015-09-12
#
#   Repeat of VClamp expt with sdia=0.3, dri=75, ca =1.0e-3, na = 3.0 

mosrun -l -b -g retsim --expt sbac_stim --sbac_file morph_sbac_168c --sbac_densfile dens_sbacc.n --sbac_densfile2 dens_sbacd.n --sb_rm 1e4 --dri 75 --sb_vs -0.049 --sb_vr -0.060 --sbarr 0 --sbac_isynanni 80 --dbp1_anpi 20 --dbp1_anpo 90 --sbac_isynanpo 60 --wac_g 0 --dSomaDia 10 --nadist 3e-3 --namid 3e-3 --cadist 1e-3 --camid 1e-3 --ksoma 3e-3 --kprox 2e-3 --kmid 2e-3 --kdist 2e-3 --sdia 0.3 --spdia 0.5 --run_vclamp 1 -d 0 >& sbac1_spaceclamp.1.0.3.0.0.3.r & 

