#  midbp
#
mosrun -l retsim --expt sbac_stim --sbac_file morph_sbac_168 --sbac_densfile dens_sbacc.n --sb_rm 1e4 --sbarr 2 --sbspac 70 --sb_vs -0.049 --sb_vr -0.060 --radincr 10 --soma_z -50 --sbac_thetax 4 --sbac_thetay 4 --predur 0.15 --vhold -0.06 --minten -0.038 --scontrast 0.003 --outward 0 --iscal 2e-9 --dri 100 --ninfo 2 --set_vclamp 0 --sdia 0.5 --dSomaDia 10 --sbac_dend1 1 --sbac_dend_cplam 0.1 --stimtype 1 --barwidth 30 --barlength 500 --velocity 2000 --stimdur 0.5 --nadist 0e-3 --namid 0e-3 --kdist 2e-3 --kmid 2e-3 --kprox 2e-3 --ksoma 3e-3 --cadist 2.5e-3 --camid 0e-3 --plotlabel sb2_midbp_ni -r 12345 --n_hpb1 0 --dbp1_cond 15e-10 --dbp1_nscale -2.1 --sbac_nscale -3.05 --sbaclm 0.1 --no_excit 1 --no_inhib 1 --sbac_isynanpo 40 --sbac_isynrngi -1120 --sbac_isynanni 55 --dbp1_anpi 15 --dbp1_anpo 60 --sbac_ithr -0.057 --stim_theta 0 --mxrot 0 --make_movie 0 --space_time 1 --frame_int 0.0002 --Vmin -0.06 --Vmax -0.045 >& sbac2_midbp_ni_ca.r &
#
#
plotmod sbac2_allbp_ca.r    | vid -c > sbac2_allbp_ca.ps
plotmod sbac2_allbp_ni_ca.r | vid -c > sbac2_allbp_ni_ca.ps
plotmod sbac2_midbp_ca.r    | vid -c > sbac2_midbp_ca.ps
plotmod sbac2_midbp_ni_ca.r | vid -c > sbac2_midbp_ni_ca.ps
#
ps2pdf sbac2_allbp_ca.ps
ps2pdf sbac2_allbp_ni_ca.ps
ps2pdf sbac2_midbp_ca.ps
ps2pdf sbac2_midbp_ni_ca.ps
