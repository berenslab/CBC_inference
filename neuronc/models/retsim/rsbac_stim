  sbac_dsi_printv_300 sbac_dsi.360.115.txt > sbac_dsi.360.115_v.txt
  sbac_dsi_printv_300 sbac_dsi.360.135.txt > sbac_dsi.360.135_v.txt
  sbac_dsi_printv_300 sbac_dsi.360.155.txt > sbac_dsi.360.155_v.txt
