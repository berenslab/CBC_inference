
#
#  surface area for sbac
#
retsim --expt surf_area --celltype sbac --sbac_file morph_sbac3c --sbac_densfile dens_sbaca.n --nvalfile nval_dsgc_sbac2.n --sb_vr -0.06 --sb_vs -0.06 --sb_rm 1e4 --sbaclm 0.1 --dri 200 --dcm 1e-6 --dendp_dia_factor 1.5 --nadist 10e-3 --namid 8e-3 --DENDD 0 --DEND 1 --DENDP 2 --SOMA 3 --HILLOCK 4 --HCK 4 --sbac_synanp 0 --sbac_synrng 0 --sbac_isynrngi 0 --sbac_isynanpi 0 --sbac_isynanpo 0 --kdist 0 --kmid 1e-3 --kprox 1e-3 --ksoma 3e-3 --cadist 0 --camid 0 --sb_rid 0 --sb_rii 0 --sb_rip 0

retsim --expt sbac_vclamp --sbac_densfile dens_sbaca.n --sbac_densfile2 dens_sbacb.n --sbac_file morph_simple_sbac --nvalfile nval_dsgc_sbac2.n --scontrast 0.01 --sbarrsiz 1 -v | vid

mosrun -l retsim --expt sbac_vclamp --sbac_file morph_simple_sbac --sbac_densfile dens_sbaca.n --sbac_densfile2 dens_sbacb.n --sb_rm 1e4 --sb_vs -0.069 --sb_vr -0.069 --predur 0.02 --vhold -0.069 --minten -0.05 --scontrast 0.015 --inward 1 --iscal 150e-12 --sbarrsiz 0.1 --complam 0.02 --dend_dia_factor 0.6 --mxrot -90 --ninfo 2 --set_vclamp 0 --plotlabel sbac_nak -r 12345 -v | vid -c > sbac_nak.ps &

mosrun -l retsim --expt sbac_vclamp --sbac_file morph_simple_sbac --sbac_densfile dens_sbaca.n --sbac_densfile2 dens_sbacb.n --sb_rm 1e4 --sb_vs -0.069 --sb_vr -0.069 --predur 0.02 --vhold -0.069 --minten -0.05 --scontrast 0.015 --inward 1 --iscal 150e-12 --sbarrsiz 0.1 --complam 0.02 --dend_dia_factor 0.6 --mxrot -90 --ninfo 2 --set_vclamp 1 --plotlabel sbac_nak_vc -r 12345 -v | vid -c > sbac_nak_vc.ps &

mosrun -l retsim --expt sbac_vclamp --sbac_file morph_simple_sbac --sbac_densfile dens_sbaca.n --sbac_densfile2 dens_sbacb.n --sb_rm 1e4 --sb_vs -0.069 --sb_vr -0.069 --predur 0.02 --vhold -0.069 --minten -0.05 --scontrast 0.015 --inward 1 --iscal 150e-12 --sbarrsiz 0.1 --complam 0.02 --dend_dia_factor 0.6 --mxrot -90 --ninfo 2 --set_vclamp 0 --plotlabel sbac_nona --_NA 99 -r 12345 -v | vid -c > sbac_nona.ps &

