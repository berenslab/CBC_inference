# 2016_0912
#
# test makestim
#  make the stim file
#
rsbac_stim_plots_cont_sineann_vhf --sb_inhib 160e-12  --set_vclamp 1 --velocity 1000 --dri 30 --sb_rm 20000 --vhold -0.06 --use_stimfile 1 --makestim 1

#  use the stim file
#
rsbac_stim_plots_cont_sineann_vhf --sb_inhib 160e-12  --set_vclamp 1 --velocity 1000 --dri 30 --sb_rm 20000 --vhold -0.04 --use_stimfile 1 --makestim 0

rsbac_stim_plots_cont_sineann_vhf --sb_inhib 160e-12  --set_vclamp 1 --velocity 1000 --dri 30 --sb_rm 20000 --vhold -0.045 --use_stimfile 1 --makestim 0

rsbac_stim_plots_cont_sineann_vhf --sb_inhib 160e-12  --set_vclamp 1 --velocity 1000 --dri 30 --sb_rm 20000 --vhold -0.05 --use_stimfile 1 --makestim 0

rsbac_stim_plots_cont_sineann_vhf --sb_inhib 160e-12  --set_vclamp 1 --velocity 1000 --dri 30 --sb_rm 20000 --vhold -0.06 --use_stimfile 1 --makestim 0

