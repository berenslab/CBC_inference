# r92sep5

nice +20

stim hz270.n
nc -t -s exponb 5  -s expona 20 hz270.n >& hz270a.r  
nc -t -s exponb 5  -s expona 10 hz270.n >& hz270b.r  
nc -t -s exponb 5  -s expona 5  hz270.n >& hz270c.r  
nc -t -s exponb 2  -s expona 5  hz270.n >& hz270d.r  
nc -t -s exponb 2  -s expona 2  hz270.n >& hz270e.r  
nc -t -s exponb 1  -s expona 2  hz270.n >& hz270f.r  
nc -t -s exponb 2  -s expona 1  hz270.n >& hz270g.r  
nc -t -s exponb 5  -s expona 2  hz270.n >& hz270h.r  
nc -t -s exponb 10 -s expona 20 hz270.n >& hz270j.r  
nc -t -s exponb 10 -s expona 2  hz270.n >& hz270k.r
nc -t -s exponb 10 -s expona 2 -s hgsize .25 hz270.n >& hz270k.25.r
nc -t -s exponb 10 -s expona 2 -s hgsize .1  hz270.n >& hz270k.1.r
nc -t -s exponb 10 -s expona 5  hz270.n >& hz270l.r  
nc -t -s exponb 5  -s conda  0  hz270.n >& hz270m.r  
nc -t -s condb  0  -s expona 5  hz270.n >& hz270n.r  

nc -t -s exponb 5 -s conda 0    -s condb 2e-9  hz270.n >& hz270ca.r
nc -t -s exponb 2 -s conda 0    -s condb 2e-9  hz270.n >& hz270cb.r
nc -t -s exponb 1 -s conda 0    -s condb 2e-9  hz270.n >& hz270cc.r
nc -t -s exponb 5 -s conda 1e-9 -s condb 2e-9  hz270.n >& hz270cd.r
nc -t -s exponb 2 -s conda 1e-9 -s condb 2e-9  hz270.n >& hz270ce.r
nc -t -s exponb 1 -s conda 1e-9 -s condb 2e-9  hz270.n >& hz270cf.r


nc -t -s exponb 5 -s expona 5 -s gjsize .1 hz270.n >& hz270da.r  
nc -t -s exponb 5 -s expona 5 -s gjsize .2 hz270.n >& hz270db.r  
nc -t -s exponb 5 -s expona 5 -s gjsize .5 hz270.n >& hz270dc.r  


nc -t -s gjsize .2 -s exponb 2   -s expona   5  hz270.n >& hz270p.r  
nc -t -s gjsize .2 -s exponb 2   -s expona 100  hz270.n >& hz270q.r  
nc -t -s gjsize .2 -s exponb 100 -s expona   5  hz270.n >& hz270r.r  

nc -t -s gjsize .2 -s exponb 2.5  -s expona   5  hz270.n >& hz270s.r  
nc -t -s gjsize .2 -s exponb 3    -s expona   5  hz270.n >& hz270t.r  
