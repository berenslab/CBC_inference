#XFM
pico:::editor.xpm:exec rxvt -T pico -n pico -e pico:exec rxvt -T pico -n pico -e pico $*
joe:::editor.xpm:exec rxvt -T joe -n joe -e joe:exec rxvt -T joe -n joe -e joe $*
vi:::scroll.xpm:exec rxvt -T vi -n vi -e vi:exec rxvt -T vi -n vi -e vi $*
xedit:::xedit.xpm:exec xedit:exec xedit $*
ez:::wordprocess.xpm:exec ez:exec ez $*
ez:::word_processor.xpm:exec ez:exec ez $*
less:::xfm_text.xpm:exec rxvt -T less -n less -e less:exec rxvt -T less -n less -e less $*
TeXView:::tex-view.xpm::exec xdvi $1
Home::.:citadel.xpm:OPEN:
Root:/:.:citadelroot.xpm:OPEN:
Trash::.trash:recycle.xpm:OPEN:shift; mv -f -b -V numbered $* $HOME/.trash
A\::/disk:a:disk.xpm:OPEN:
B\::/disk:b:floppy.xpm:OPEN:
